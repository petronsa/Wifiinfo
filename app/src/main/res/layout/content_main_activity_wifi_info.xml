<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="company.petron.wifiinfo.MainActivity_wifi_info"
    tools:showIn="@layout/activity_main_activity_wifi_info">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:weightSum="1">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab"
                android:background="@color/accent" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/estado"
                android:id="@+id/txtEstado"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
        </LinearLayout>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#333"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab1"
                android:background="@color/accent" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/ip"
                android:id="@+id/txtIP"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
        </LinearLayout>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#333"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab2"
                android:background="@color/accent" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/ssid"
                android:id="@+id/txtSSID"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
        </LinearLayout>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#333"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab4"
                android:background="@color/accent" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/ssid_oculto"
                android:id="@+id/txtSSIDOculto"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
        </LinearLayout>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#333"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab5"
                android:background="@color/accent" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/bssid"
                android:id="@+id/txtBSSID"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
        </LinearLayout>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#333"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab6"
                android:background="@color/accent" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/mac"
                android:id="@+id/txtMAC"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
        </LinearLayout>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#333"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab7"
                android:background="@color/accent" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/velocidad"
                android:id="@+id/txtVelocidad"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
        </LinearLayout>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#333"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab8"
                android:background="@color/accent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/rssi"
                android:id="@+id/txtRSSI"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
            <view
                class="android.view.View"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:id="@+id/row_tab9"
                android:background="@color/accent" />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/señal"
                android:id="@+id/txtseñal"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="@color/primary_light"
                android:padding="10dp"
                android:textColor="@color/primary_text"
                android:ellipsize="end"
                android:maxLines="1" />
        </LinearLayout>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000"/>

        <view
            class="android.view.View"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#333"/>




    </LinearLayout>

    <com.startapp.android.publish.banner.Banner
        android:id="@+id/startAppBanner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true" />
</RelativeLayout>
